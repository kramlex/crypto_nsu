   "source": [
    "import BitVector as bv\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [],
   "source": [
    "def mebius(bvec):\n",
    "    [left, right] = bvec.divide_into_two()\n",
    "    if(bvec.length() == 2):\n",
    "        tmp = left.deep_copy()\n",
    "        tmp += (left ^ right)\n",
    "        return tmp\n",
    "    else:\n",
    "        l = mebius(left).deep_copy()\n",
    "        r = mebius(right).deep_copy()\n",
    "        tmp = l.deep_copy()\n",
    "        tmp += (l ^ r)\n",
    "        return  tmp\n",
    "    \n",
    "def inc(bvec):\n",
    "    return bv.BitVector(intVal = int(bvec)+1, size = bvec.length())\n",
    "def newVec(val, vecSize):\n",
    "    return bv.BitVector(intVal = val, size = vecSize)\n",
    "def allVectorsOfRange(n):\n",
    "    vectors = [newVec(i,n) for i in range(2**n)]\n",
    "    return vectors\n",
    "def newVecFromBitList(bitset):\n",
    "    return bv.BitVector(bitlist = bitset)\n",
    "\n",
    "def dot_product(v1,v2):\n",
    "    res = 0\n",
    "    for i in range(len(v1)):\n",
    "        res += (v1[i] * v2[i]) \n",
    "    return res % 2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "metadata": {},
   "outputs": [],
   "source": [
    "values = [1,2,3,4,6,7,5,0]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 78,
   "metadata": {},
   "outputs": [],
   "source": [
    "class Task:\n",
    "    def __init__(self,values):\n",
    "        self.n = int(np.log2(len(values)))\n",
    "        self.size = len(values)\n",
    "        self.values = [bv.BitVector(intVal=int(i),size=self.n) for i in values]\n",
    "        self.dict = {}\n",
    "        self.dictInit()\n",
    "    \n",
    "        \n",
    "    def dictInit(self):\n",
    "        for i in range(self.size):\n",
    "            self.dict[str(newVec(i,self.n))] = newVec(int(self.values[i]), self.n)\n",
    "        print('x -> F(x)')\n",
    "        for key in self.dict:\n",
    "            print(key, '->', self.dict[key])\n",
    "            \n",
    "    def mebiusAll(self):\n",
    "        print('Mebius')\n",
    "        self.mebius = []\n",
    "        for i in range(self.n):\n",
    "            tmp = []\n",
    "            for j in range(self.size):\n",
    "                tmp.append(self.values[j][i])\n",
    "            tmpVec = newVecFromBitList(tmp)\n",
    "            self.mebius.append( [tmpVec, mebius(tmpVec)] )\n",
    "        for i in self.mebius:\n",
    "            print(i[0], '->', i[1])\n",
    "            \n",
    "    def printANF(self):\n",
    "        print('anf')\n",
    "        sm = ''\n",
    "        for i in range(self.size):\n",
    "            tmp = []\n",
    "            for j in range(self.n):\n",
    "                tmp.append(self.mebius[j][1][i])\n",
    "            tmpArr = np.array(tmp)\n",
    "            if i == 0:\n",
    "                print(1)\n",
    "            else:\n",
    "                monom = bv.BitVector(intVal = i, size = self.n)\n",
    "                monom_str = ''\n",
    "                for k in range(self.n):\n",
    "                    if monom[k] == 1:\n",
    "                        monom_str += 'x'+str(k+1)\n",
    "                print(monom_str)\n",
    "            print(tmpArr)\n",
    "            if i != self.size - 1:\n",
    "                print(' + ')\n",
    "                \n",
    "    def F(self,x):\n",
    "        return self.dict[str(x)]\n",
    "        \n",
    "    def Wf(self,u,v):\n",
    "        xs = allVectorsOfRange(self.n)\n",
    "        sum = 0\n",
    "        for x in xs:\n",
    "            tmp = (-1)**( (dot_product(v,self.F(x)) + dot_product(u,x)) % 2 )\n",
    "            print('v: ',v ,' | F(x): ',self.F(x),' | u: ', u ,' | x: ',x,' | <v,F(x)>: ',(dot_product(v,self.F(x))) , ' | <u,x>: ',dot_product(u,x) , ' | <v,F(x)> xor <u,x>: ',((dot_product(v,self.F(x))) + dot_product(u,x)) % 2 ,' ->', tmp)\n",
    "            sum += tmp\n",
    "        return sum\n",
    "    \n",
    "    def Nf(self):\n",
    "        us = allVectorsOfRange(self.n)\n",
    "        vs = allVectorsOfRange(self.n)\n",
    "        Ws = []\n",
    "        for u in us:\n",
    "            for v in vs:\n",
    "                if(int(v) != 0):\n",
    "                    tmp = self.Wf(u,v)\n",
    "                    Ws.append(tmp)\n",
    "                    \n",
    "                    print('*********')\n",
    "        Ws = np.array(Ws)\n",
    "        print('N[F] = ',2**(self.n-1) - 0.5 * max(Ws))\n",
    "        \n",
    "    def DDT(self):\n",
    "        sa = allVectorsOfRange(self.n)\n",
    "        sb = allVectorsOfRange(self.n)\n",
    "        sx = allVectorsOfRange(self.n)\n",
    "        ddt = np.zeros((int(2**self.n), int(2**self.n)))\n",
    "        maxCounter = 0\n",
    "        for b in sa:\n",
    "            for a in sb:\n",
    "                if int(a) == 0:\n",
    "                    continue\n",
    "                counter = 0\n",
    "                for x in sx:\n",
    "                    if self.F(x) ^ self.F(x ^ a) == b:\n",
    "                        counter += 1\n",
    "                        \n",
    "                ddt[int(a), int(b)] = counter\n",
    "                if(counter > maxCounter):\n",
    "                    maxCounter = counter\n",
    "            \n",
    "         \n",
    "        print(ddt)\n",
    "        print('Порядок дифф. равномерности = ',maxCounter)\n",
    "                    \n",
    "                "
   ]
  },
